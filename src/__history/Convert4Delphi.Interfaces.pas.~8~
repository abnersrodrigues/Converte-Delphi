unit Convert4Delphi.Interfaces;

interface

Uses
  System.SysUtils, System.Classes, FireDAC.Comp.Client;

Type

  iConvert4Delphi = interface
    function Convert(AQuery: TFDQuery; ADest: String): iConvert4Delphi;
    function Import(AQuery: TFDQuery; AOrig: String):  iConvert4Delphi;
  end;

  iConvert = interface
    function Executar: iConvert;
  end;

  iImport = interface
    function Executar: iImport;
  end;

implementation

end.
